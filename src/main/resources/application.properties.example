spring.application.name=Hello Turksat

spring.datasource.url=<your-db-url>
spring.datasource.username=<your-db-username>
spring.datasource.password=<your-db-password>
spring.jpa.properties.hibernate.default_schema=<your-db-schema>
spring.datasource.driver-class-name=<your-db-driver>

spring.jpa.hibernate.ddl-auto=update
spring.jpa.show-sql=true
spring.jpa.properties.hibernate.format_sql=true

jwt.secretKey=<your-db-secretKey>
jwt.expirationTime=<jwt-expirationTime>
jwt.refresh.expirationTime=<refresh-expirationTime>